<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="700.0" prefWidth="550.0" spacing="20.0" stylesheets="@theme.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Controllers.InfoUnitController">
   <children>
      <Label text="Информация по всем аккаунтам">
         <VBox.margin>
            <Insets top="40.0" />
         </VBox.margin></Label>
      <Button mnemonicParsing="false" onAction="#getAllInfo" text="Получить">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <TextArea fx:id="infoAll" prefHeight="500.0" prefWidth="510.0">
         <font>
            <Font size="14.0" />
         </font></TextArea>
      <HBox prefHeight="33.0" prefWidth="502.0">
         <children>
            <Button mnemonicParsing="false" onAction="#switchToInfoUnit" text="Назад">
               <HBox.margin>
                  <Insets left="160.0" />
               </HBox.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#switchToMenu" text="Главное меню">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </Button>
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
